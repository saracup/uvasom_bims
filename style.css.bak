/* Defaults
------------------------------------------------------------ */
html, body {overflow-x:hidden;}
body,
h1,
h2,
h2 a,
h2 a:visited,
h3,
h4,
h5,
h6,
p,
select,
textarea {
	color: #333;
	font-family: Georgia,Times,serif;
	font-size: 16px;
	font-weight: 300;
	line-height: 1.5625;
	margin: 0;
	padding: 0;
	text-decoration: none;
}
p,select,textarea,.entry-content ul,.entry-content ol {
font-size:0.9rem;
font-family: Georgia,Times,serif;

}
strong {
	font-weight:700;
}

blockquote,
input,
select,
textarea,
.author-box,
.breadcrumb,
.sticky,
.taxonomy-description,
.wp-caption {
	background:none;
	border: none;
}
#content input,
#content select,
#content textarea{
	border: 1px dotted #666;
	background:#f7f7f7;
	color:#333;
}
#content textarea{
padding-right:0;
}
.comment-form #submit {
	background:#666;
	color:#fff;
}
.comment-form #submit:hover {
	background:#336699;
}

input,
label,
select,
textarea {
	color: #fff;
	font-family: "proxima-nova-condensed",Helvetica,Verdana,sans-serif;
	font-size: 1.0em;
	font-weight: 300;
}

#footer-widgets input,#footer-widgets label, #footer-widgets select, #footer-widgets textarea {
color: #333;
font-size:1.2em;
}
#footer-widgets input.s {
	border:1px dotted #666;
	background:#fff;
	width:150px;
}
#footer-widgets .searchsubmit {
	border:1px dotted #666;
	color:#fff;
	background:#ccc;
}
.entry-content input,
.entry-content label,
.entry-content select,
.entry-content textarea {
	color: #000;
	font-family: Georgia,serif;
}
label,.entry-content label{
border:none;
}

/*SIDEBAR INPUTS*/
.sidebar input,
.sidebar label,
.sidebar select,
.sidebar textarea {
	color: #333;
}
.sidebar textarea {
  border: 1px solid #ccc;
}
.sidebar select {
	width:100%;
	margin:8px 0 3px 0;
	background:#f7f7f7;
	background-image:none;
	border:1px solid #ccc;
}
.sidebar input[type=text] {
	width:100%;
	border:1px solid #ccc;
}
.sidebar input[type=submit] {
	width:100%;
	border:1px dotted #666;
	background:#d7d7d7;
	margin-top:8px;
}
.sidebar input[type=submit]:hover {
	color:#fff;
	background:#666666;
}

#header input#s {
	color: #000;
	font-family: "proxima-nova-condensed",Helvetica,Verdana,sans-serif;
	font-size: 1.0em;
	font-weight: 300;
	background:#fff;
}
li,
ol,
ul {
	margin: 0;
	padding: 0;
}
ol li {
	list-style-type: decimal;
}
