/* Hyperlinks
------------------------------------------------------------ */
a,
a:visited {
	color: #175D8C;
	text-decoration: none;
}
a:hover {
	text-decoration: underline;
}
a img {
	border: none;
}
/* Body
------------------------------------------------------------ */
body {
	background-color: transparent;
}
/* Wrap
------------------------------------------------------------ */
#wrap {
	background:none;
	margin: 0;
	width: 100%;
	padding: 0;
}
/* Header
------------------------------------------------------------ */
#header {
	margin: 0 auto;
min-height: 60px;
max-width: 1140px;
background: none;
clear:both;
position:relative;
padding-top:15px;
}
#title-area {
	float: left;
	margin:0;
	overflow: hidden;
	padding: 0;

}
#title {
	font-family: "adobe-caslon-pro",Georgia,serif;
	font-size: 2.2em;
	line-height: 1.05;
	text-transform: none;
	font-weight:100;
	padding-top:5px;
}
#title a,
#title a:hover {
	color: #fff;
	text-decoration: none;
}
#description {
	font-size: 14px;
	padding: 0;
}
#header .widget-area {
	float: right;
	padding: 25px 20px 0 0;

}
#header .widget-area fieldset {
	border:none;
padding:0;
}
#header .widget-area label {
	font-family:"proxima-nova-condensed",Helvetica,Verdana,sans-serif;
	font-weight:300;
	color:#fff;
	font-size:0.9em;
}
#logos {
background-image:url(images/rotunda2x.png);
background-repeat:no-repeat;
background-size:42px 42px;
padding-left:55px;
width:400px;
display:block;
height:55px;
float:left;
clear:none;
margin-top:8px;
}
#header a.uva-logo {

float:left;
clear:right;
max-width:182px;
padding:0;
margin:0;
max-height:18px;
line-height:1.0
}
#header a.som-logo {

float:left;
clear:none;
padding:0;
margin:0;
line-height:1.0;
}
#uvasom_searchbutton {
	display:none;
}
.clearfix {
	width:100%;
	clear:both;
	height:0;
	display:block;
}
#header .wrap {
	padding:0;
	margin:0;
}
/********** hide typekit fonts while loading ***********/
.wf-loading {
	visibility:hidden;
}
/* Image Header - Partial Width
------------------------------------------------------------ */
.header-image #title-area,
.header-image #title,
.header-image #title a {
	display: block;
	float: left;
	height: 100px;
	overflow: hidden;
	padding: 0;
	text-indent: -9999px;
	max-width: 400px;
}
.header-image #description {
	display: block;
	overflow: hidden;
}
/* Image Header - Full Width
------------------------------------------------------------ */
.header-full-width #title-area,
.header-full-width #title,
.header-full-width #title a {
	max-width: 100%;
}
 .custom-header #title-area
 {
max-width:80%;
padding-left: 15px;
padding-top:15px;
}
.custom-header #header {
	height:120px;
	padding-top:0px;
	padding-bottom:0px;
}
.custom-header #header #uvasom_sitesearch {
	margin-top:20px;
}
.custom-header #header, #header {
	background-size:100%;
}
/*Tertiary site header*/
#uvasom_rootsite {
	font-family: 'proxima-nova-condensed',sans;
	font-size: 1.1rem;
	padding:.5rem 0 0 0;
}
.custom-header #uvasom_rootsite {
	padding: .5rem 0 0 1.0rem;

}
.site-section_tertiary.custom-header #header #title-area {
padding-top: .5rem;
}
#uvasom_rootsite a {
	text-transform:uppercase;
	color:#fff;
}
/*end tertiary header*/
