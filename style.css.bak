blockquote {
	background: url(images/blockquote.png) no-repeat;
	border: none;
	margin: 5px 5px 20px;
	padding: 25px 30px 5px 70px;
}
blockquote p {
	color: #666;
	font-style: italic;
}
blockquote:before {
display: block;
content: "\201C";
font-size: 3.0em;
position: absolute;
left: -20px;
top: -20px;
color: #7a7a7a;
}
