/************************************************************************************************
GENESIS ORIGINAL LAYOUT STYLES*/

/* 04a - Structure ----------- */

#wrap {
	margin: 0 auto;
	max-width: 1140px;
}

#inner {
	clear: both;
	overflow: hidden;
	padding-top: 40px;
	padding-top: 2.5rem;
	background:#fff;
	max-width: 1010px;
	margin: 0 auto;
	padding:0;
	padding:20px 65px 20px 65px;
}

/* 04b - 1152px CSS Grid ----------- */
#content {
}


#header .widget-area {
	width: 30.555555555%; /* 352px / 1152px */
}
#sidebar-alt {
	width: 25%; /* 176px / 1152px */
	padding:0;

}
.sidebar-content-sidebar .sidebar {
	width: 32%; /* 352px / 928px */
	padding:0 2%;
}

.content-sidebar-sidebar #content,
.sidebar-content-sidebar #content,
.sidebar-sidebar-content #content {
	width:62%; /* 528px / 928px */
}

.content-sidebar #content,
.sidebar-content #content
{
	width: 70%; /* 752px / 1152px */
}

.content-sidebar-sidebar #content-sidebar-wrap,
.sidebar-content-sidebar #content-sidebar-wrap,
.sidebar-sidebar-content #content-sidebar-wrap {
	width: 72%; /* 928px / 1152px */
}

.content-sidebar #content-sidebar-wrap,
.full-width-content #content,
.full-width-content #content-sidebar-wrap,
.sidebar-content #content-sidebar-wrap {
	width: 100%; /* 1152px / 1152px */
}

.sidebar-content #sidebar,
#content,
#content-sidebar-wrap,
#footer .gototop,
#sidebar-alt {
	float: left;
	width:27%;
}
.sidebar-content-sidebar #sidebar-alt {
	width:23%;
}
.content-sidebar-sidebar #sidebar-alt,
.sidebar,
.sidebar-content #content,
.sidebar-content-sidebar #content-sidebar-wrap,
.sidebar-sidebar-content #content,
.sidebar-sidebar-content #content-sidebar-wrap,
#footer .creds,
#header .widget-area {
	float: right;
}

/******** home pages have narrower padding for larger image display **********/
body.home.sidebar-content-sidebar #inner,
body.home.full-width-content #inner
 {
	padding:20px 0;
	max-width: 1140px;
}
body.home.full-width-content.empty #inner {
padding:0px;
}
body.home.sidebar-content-sidebar .sidebar {
	width:32%;
	padding:0 2%;
}
body.home.sidebar-content-sidebar #sidebar-alt {
	width:22%;
	padding:0 2%;
}
body.home.full-width-content .entry-content,
body.home.full-width-content h1.entry-title {
margin-left:50px;
margin-right:50px;
}
body.home.full-width-content h1.entry-title {
padding-top:20px;
max-width: 1050px;
}
body.home.content-sidebar #content,
body.home.sidebar-content #content
{
	width: 70%; /* 752px / 1152px */
}

